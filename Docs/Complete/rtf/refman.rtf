\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid {
{\b 
static bool PhotonNetwork.JoinRoom (string  {\i roomName}, bool  {\i createIfNotExists}){\f2 [static]}}}
